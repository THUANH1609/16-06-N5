<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_khach_hang_code" model="ir.sequence">
            <field name="name">Mã Khách Hàng Tự Động</field>
            <field name="code">khach_hang.code</field>
            <field name="prefix">KH</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_khach_hang_tiem_nang" model="ir.sequence">
            <field name="name">Mã Lead Tự Động</field>
            <field name="code">khach_hang_tiem_nang.code</field>
            <field name="prefix">LEAD</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_bao_gia" model="ir.sequence">
            <field name="name">Mã Báo Giá Tự Động</field>
            <field name="code">bao_gia.code</field>
            <field name="prefix">BG</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hop_dong" model="ir.sequence">
            <field name="name">Mã Hợp Đồng Tự Động</field>
            <field name="code">hop_dong.code</field>
            <field name="prefix">HD</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
